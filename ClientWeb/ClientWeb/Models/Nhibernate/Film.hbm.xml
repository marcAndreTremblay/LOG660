<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="ClientWeb"
                   namespace="ClientWeb.Models">

  <class name="Film" table="Film" dynamic-update="true">
    <cache usage="read-write" />
    <id name="Id" column="FilmId" type="int">
      <generator class="native" />
    </id>
    <property name="AnneeDeSortie" column="annee" />
    <property name="Pays" />
    <property name="LangueOriginale" />
    <property name="Genres" />
    <property name="Resume" column="ResumeFilm" />
    <property name="DureeMinutes" />
  </class>
</hibernate-mapping>